service dhcpcd_tiwlan0 /system/bin/dhcpcd
    disabled
    oneshot
service dhcpcd_bnep0 /system/bin/dhcpcd
    disabled
    oneshot
service dhcpcd_usbeth0 /system/bin/dhcpcd
    disabled
